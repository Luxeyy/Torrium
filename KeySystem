local TweenService = game:GetService("TweenService")
local Plr = game.Players.LocalPlayer
function MakeTween(Duration, Style, Direction, Reptitions, Reverse, DelayAmount)
	local tweenInfo = TweenInfo.new(
		Duration, -- Duration
		Enum.EasingStyle[Style], -- Easing style
		Enum.EasingDirection[Direction], -- Easing direction
		Reptitions, -- Number of repetitions (-1 means infinite)
		Reverse, -- Reverses on repeat
		DelayAmount -- Delay between repetitions
	)

	return tweenInfo
end

--[[
local TweenInfo1 = MakeTween(0.25, "Linear", "Out", 0, false, 0)
local MoveLogoToSide = TweenService:Create(script.Parent.KeySystemBackground.Logo, TweenInfo1, {Position = UDim2.new(0.431, 0,0.44, 0)})

wait(2)
--]]
